<project chip="LM3S8962"
         postBuild="arm-none-eabi-objcopy -O binary ${BuildArtifactFileName} ${BuildArtifactFileBaseName}.bin"
         type="LMI Basic C project"
         vendor="LMI">
  <import src=".">
    <exclude>{(Makefile|ccs|codered|ewarm|gcc|rvmdk|sourcerygxx)}</exclude>
    <exclude>{startup_(ewarm|gcc|rvmdk|sourcerygxx|ccs).(c|S)}</exclude>
    <exclude>{.*\.(ewd|ewp|eww|icf|Opt|sct|Uv2|uvopt|uvproj|xml|ld|cmd)}</exclude>
  </import>
  <import src="..">
    <copy>drivers/rit128x96x4.c</copy>
    <copy>drivers/rit128x96x4.h</copy>
  </import>
  <import src="../../..">
    <copy>utils/locator.c</copy>
    <copy>utils/lwiplib.c</copy>
    <copy>utils/ustdlib.c</copy>
    <copy>third_party/lwip-1.3.1/apps/httpserver_raw/httpd.c</copy>
  </import>
  <import src=".">
    <copy>enet_io_codered.ld</copy>
  </import>
  <requires>
    <value>inc</value>
    <value>driverlib</value>
    <value>third_party</value>
    <value>utils</value>
  </requires>
  <setting id="assembler.def">
    <value>codered</value>
  </setting>
  <setting id="compiler.def"
           buildType="Debug"
           mode="replace">
    <value>DEBUG</value>
  </setting>
  <setting id="compiler.def"
           buildType="Release"
           mode="replace">
    <value>NDEBUG</value>
  </setting>
  <setting id="compiler.def">
    <value>__CODE_RED</value>
    <value>codered</value>
    <value>PART_LM3S8962</value>
  </setting>
  <setting id="compiler.opt"
           buildType="Debug">
    <value>-Os</value>
  </setting>
  <setting id="compiler.opt"
           buildType="Release">
    <value>-Os</value>
  </setting>
  <setting id="compiler.flags"
           buildType="Debug">
    <value>-Os</value>
  </setting>
  <setting id="compiler.flags"
           buildType="Release">
    <value>-Os</value>
  </setting>
  <setting id="linker.script">
    <value>../enet_io_codered.ld</value>
  </setting>
  <setting id="linker.libs">
    <value>driver</value>
    <value>c</value>
    <value>gcc</value>
  </setting>
  <setting id="linker.paths"
           buildType="Debug">
    <value>${workspace_loc:/driverlib/Debug}</value>
  </setting>
  <setting id="linker.paths"
           buildType="Release">
    <value>${workspace_loc:/driverlib/Release}</value>
  </setting>
  <setting id="compiler.inc">
    <value>${workspace_loc:/}</value>
    <value>..</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/apps}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/apps/httpserver_raw}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/ports/stellaris/include}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/src/include}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/src/include/ipv4}</value>
  </setting>
  <setting id="assembler.inc">
    <value>${workspace_loc:/}</value>
    <value>..</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/apps}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/apps/httpserver_raw}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/ports/stellaris/include}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/src/include}</value>
    <value>${workspace_loc:/third_party/lwip-1.3.1/src/include/ipv4}</value>
  </setting>
</project>
